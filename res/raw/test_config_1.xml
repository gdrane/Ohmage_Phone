<?xml version="1.0" encoding="UTF-8"?>
<campaign>
  <campaignName>TEST</campaignName>
  <campaignVersion>0.1</campaignVersion>
  <serverUrl>https://dev.andwellness.org/</serverUrl>

  <surveys>
    <survey>
      <id>testSurvey</id>
      <title>First Test Survey</title>
      <description>Every survey must have a description.</description>
      <submitText>Every survey must include submitText.</submitText>
      <showSummary>false</showSummary>
      <editSummary>false</editSummary>
      <summaryText>Every survey must include summaryText.</summaryText>
      <anytime>true</anytime>
    
      <contentList>
      
        <prompt>
          <id>singleTest</id>
          <displayType>measurement</displayType>
          <displayLabel>Single</displayLabel>
          <promptText>Select one:</promptText>
          <abbreviatedText>abbreviatedText</abbreviatedText>
          <promptType>single_choice</promptType>
          <properties>
            <property>
              <key>0</key>
              <label>A</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>B</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>C</label>
              <value>2</value>
            </property>
            <property>
              <key>3</key>
              <label>D</label>
              <value>3</value>
            </property>
          </properties>
          <skippable>true</skippable>
          <skipLabel>skipLabel</skipLabel>
        </prompt>
        
        <prompt>
          <id>multiTest</id>
          <displayType>measurement</displayType>
          <displayLabel>Select all that apply:</displayLabel>
          <promptText>Select all that apply:</promptText>
          <abbreviatedText>abbreviatedText</abbreviatedText>
          <promptType>multi_choice</promptType>
          <condition>singleTest == 0</condition>
          <properties>
            <property>
              <key>0</key>
              <label>Z</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>Y</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>X</label>
              <value>2</value>
            </property>
            <property>
              <key>3</key>
              <label>W</label>
              <value>3</value>
            </property>
            <property>
              <key>4</key>
              <label>V</label>
              <value>4</value>
            </property>
            <property>
              <key>5</key>
              <label>U</label>
              <value>5</value>
            </property>
          </properties>
          <skippable>false</skippable>
        </prompt>
        
        <prompt>
          <id>multiCustomTest</id>
          <displayType>measurement</displayType>
          <displayLabel>Select all that apply:</displayLabel>
          <promptText>Select all that apply:</promptText>
          <abbreviatedText>abbreviatedText</abbreviatedText>
          <promptType>multi_choice_custom</promptType>
          <properties>
            <property>
              <key>0</key>
              <label>Z</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>Y</label>
              <value>1</value>
            </property>
          </properties>
          <skippable>false</skippable>
        </prompt>
        
        <prompt>
          <id>singleCustomTest</id>
          <displayType>measurement</displayType>
          <displayLabel>Select one:</displayLabel>
          <promptText>Select one:</promptText>
          <abbreviatedText>abbreviatedText</abbreviatedText>
          <promptType>single_choice_custom</promptType>
          <properties>
            <property>
              <key>0</key>
              <label>Z</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>Y</label>
              <value>1</value>
            </property>
          </properties>
          <skippable>false</skippable>
        </prompt>
        
        <prompt>
          <id>numberTest</id>
          <displayType>measurement</displayType>
          <displayLabel>How many?</displayLabel>
          <promptText>How many?</promptText>
          <abbreviatedText>abbreviatedText</abbreviatedText>
          <promptType>number</promptType>
          <properties>
            <property>
              <key>min</key>
              <label>1</label>
            </property>
            <property>
              <key>max</key>
              <label>100</label>
            </property>
          </properties>
          <skippable>true</skippable>
          <skipLabel>skipLabel</skipLabel>
        </prompt>
        
        <prompt>
          <id>hoursTest</id>
          <displayType>measurement</displayType>
          <displayLabel>How long ago?</displayLabel>
          <promptText>How long ago?</promptText>
          <abbreviatedText>abbreviatedText</abbreviatedText>
          <promptType>hours_before_now</promptType>
          <properties>
            <property>
              <key>min</key>
              <label>0</label>
            </property>
            <property>
              <key>max</key>
              <label>24</label>
            </property>
          </properties>
          <skippable>false</skippable>
        </prompt>
        
        
        <prompt>
          <id>textTest</id>
          <displayType>measurement</displayType>
          <displayLabel>What?</displayLabel>
          <promptText>What?</promptText>
          <abbreviatedText>abbreviatedText</abbreviatedText>
          <promptType>text</promptType>
          <properties>
            <property>
              <key>min</key>
              <label>1</label>
            </property>
            <property>
              <key>max</key>
              <label>500</label>
            </property>
          </properties>
          <skippable>false</skippable>
        </prompt>
        
        <prompt>
          <id>photoTest</id>
          <displayType>measurement</displayType>
          <displayLabel>photo</displayLabel>
          <promptText>Take a photo:</promptText>
          <abbreviatedText>abbreviatedText</abbreviatedText>
          <promptType>photo</promptType>
          <properties>
            <property>
              <key>res</key>
              <label>500</label>
            </property>
          </properties>
          <skippable>false</skippable>
        </prompt>        
        
      </contentList>
    
    </survey>
  </surveys>
</campaign>